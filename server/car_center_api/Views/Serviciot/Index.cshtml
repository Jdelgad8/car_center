@model IEnumerable<car_center_api.Models.Serviciot>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VlrMin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VlrMax)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DurMin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodTipoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VlrMin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VlrMax)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DurMin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodTipoNavigation.Descripcion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodSer">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodSer">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodSer">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
