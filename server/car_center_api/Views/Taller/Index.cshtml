@model IEnumerable<car_center_api.Models.Taller>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NroCel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FecReg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodZonaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NroCel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FecReg)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodZonaNavigation.NomZona)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodTaller">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodTaller">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodTaller">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
